doctype html
html(ng-app="branch" ng-strict-di)
  head
    base(href="#!")
    title(ng-bind="headTitle") Qlik Branch 
    meta(name="description" content="{{metaDesc}}")
    meta(name="keywords" content="{{metaKeys}}")
    meta(name="fragment" content="!")
    meta(name="og:title" content="{{headTitle}}")
    meta(name="og:description" content="{{metaDesc}}")
    meta(name="og:image" content="{{metaImage}}")
    meta(name="twitter:card" content="summary")
    if twitterHandle != ""
      meta(name="twitter:site" content=twitterHandle)
    meta(name="twitter:title" content="{{headTitle}}")
    meta(name="twitter:description" content="{{metaDesc}}")
    meta(name="twitter:image" content="{{metaImage}}")

    link(rel="stylesheet" href="/bower_components/bootstrap/dist/css/bootstrap.min.css")    
    link(rel="stylesheet" href="/bower_components/font-awesome/css/font-awesome.min.css")    
    link(rel="stylesheet" href="/bower_components/summernote/dist/summernote.css")  
    link(rel="stylesheet" href="/bower_components/visualcaptcha.angular/visualcaptcha.css")  
    link(rel="stylesheet" href="/css/main.css")    
    script(src="https://use.typekit.net/rqj4zil.js")
    script(type='text/javascript').
      try{Typekit.load({ async: true });}catch(e){}
    script(src="/bower_components/jquery/dist/jquery.min.js")
    script(src="/bower_components/d3/d3.min.js")
    script(src="/bower_components/angular/angular.js")
    script(src="/bower_components/angular-resource/angular-resource.js")
    script(src="/bower_components/angular-ui-router/release/angular-ui-router.min.js")    
    script(src="/bower_components/angular-sanitize/angular-sanitize.js") 
    script(src="/bower_components/angular-sanitize/textAngular-sanitize.js") 
    script(src="/bower_components/summernote/dist/summernote.min.js")    
    script(src="/node_modules/marked/marked.min.js")
    script(src="/qsocks/qsocks.bundle.js")
    if mode == "debug"
      script(src="/debug/app.js")
      script(src="/debug/controllers/admin.js")
      script(src="/debug/controllers/moderator.js")
      script(src="/debug/controllers/auth.js")
      script(src="/debug/controllers/blog.js")
      script(src="/debug/controllers/discussion.js")
      script(src="/debug/controllers/resource.js")
      script(src="/debug/controllers/comment.js")
      script(src="/debug/controllers/home.js")
      script(src="/debug/controllers/moderation.js")
      script(src="/debug/controllers/project.js")
      script(src="/debug/controllers/user.js")
      script(src="/debug/directives/comments.js")
      script(src="/debug/directives/confirm-dialog.js")
      script(src="/debug/directives/header.js")
      script(src="/debug/directives/footer.js")
      script(src="/debug/directives/moderation.js")
      script(src="/debug/directives/rating.js")
      script(src="/debug/directives/subscribe.js")
      script(src="/debug/directives/notification-dialog.js")      
      script(src="/debug/directives/search-filter.js")
      script(src="/debug/directives/search-input.js")
      script(src="/debug/directives/search-results.js")    
      script(src="/debug/directives/branchtree.js")
      script(src="/debug/directives/fallbackimage.js") 
      script(src="/debug/services/picklists.js")
      script(src="/debug/services/publisher.js")
      script(src="/debug/services/lastError.js")
      script(src="/debug/services/result-handler.js")
      script(src="/debug/services/search-exchange.js")
      script(src="/debug/services/user-manager.js")   
      script(src="/debug/services/templater.js")      
    else
      script(src="/js/app.min.js")
      meta(content="text/html charset=utf-8" http-equiv="Content-Type")
      meta(name="viewport" content="width=device-width, initial-scale=1")
